# Base URL and version for API Gateway integration
VITE_API_URL=http://localhost:3000
VITE_API_VERSION=v1

# Authentication Configuration
# Azure AD B2C OAuth2 settings for secure user authentication
VITE_AUTH_CLIENT_ID=your_client_id_here
VITE_AUTH_AUTHORITY=https://your-tenant.b2clogin.com/your-tenant.onmicrosoft.com/B2C_1_signin
VITE_AUTH_REDIRECT_URI=http://localhost:5173
VITE_AUTH_KNOWN_AUTHORITIES=your-tenant.b2clogin.com

# Map Configuration
# Mapbox GL JS access token for aircraft tracking visualization
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# WebSocket Configuration
# Real-time updates endpoint for aircraft positions and trip status
VITE_WEBSOCKET_URL=ws://localhost:3000